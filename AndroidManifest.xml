<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.devortex.vortextoolbox"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="10" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true">
        <activity android:name=".Activity.VorteXToolBox"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.VorteXToolBox"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CarrierText" android:label="@string/carrier_text"></activity>
        <activity android:name=".Activity.StartupScript"></activity>
        <activity android:name=".Activity.BatteryIconSelector">
        </activity>
        <activity android:name=".Activity.PowerBoostScript"></activity>
        <activity android:name=".Activity.PowerBoost"></activity>
        <activity android:name=".Activity.Governors"></activity>
        <activity android:name=".Activity.PowerBoostCWR"></activity>
        <activity android:name=".Activity.ThrottleSelector"></activity>

    </application>
</manifest>